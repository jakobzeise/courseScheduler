<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jakobzeise.coursescheduler">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CourseScheduler">
        <activity
            android:name=".view.adds.AddCourseActivity"
            android:exported="false" />
        <activity
            android:name=".view.dataclassviews.TermActivity"
            android:exported="false" />
        <activity
            android:name=".view.dataclassviews.MentorActivity"
            android:exported="false" />
        <activity
            android:name=".view.dataclassviews.AssessmentActivity"
            android:exported="false" />
        <activity
            android:name=".view.dataclassviews.CourseActivity"
            android:exported="false" />
        <activity
            android:name=".view.adds.AddMentorActivity"
            android:exported="false" />
        <activity
            android:name=".view.adds.AddTermActivity"
            android:exported="false" />
        <activity
            android:name=".view.lists.MentorListActivity"
            android:exported="false" />
        <activity
            android:name=".view.lists.AssessmentListActivity"
            android:exported="false" />
        <activity
            android:name=".view.lists.CourseListActivity"
            android:exported="false" />
        <activity
            android:name=".view.lists.TermListActivity"
            android:exported="false" />
        <activity
            android:name=".view.dataclassviews.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>